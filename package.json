{
  "name": "concurrent-manager",
  "author": "Sutan Gading Fadhillah Nasution",
  "version": "0.1.1",
  "license": "MIT",
  "description": "A simple and fast way to manage concurrent promise tasks.",
  "homepage": "https://github.com/gadingnst/concurrent-manager",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gadingnst/concurrent-manager.git"
  },
  "bugs": {
    "url": "https://github.com/gadingnst/concurrent-manager/issues"
  },
  "keywords": [
    "concurrent",
    "manager",
    "async",
    "promise",
    "await",
    "asynchronous",
    "async-await",
    "async-promise",
    "async-await-promise",
    "queue",
    "queue-manager"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "example": "ts-node test/example.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/concurrent-manager.esm.js",
  "size-limit": [
    {
      "path": "dist/concurrent-manager.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/concurrent-manager.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "ts-node": "^10.8.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2"
  }
}
